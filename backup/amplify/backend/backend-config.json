{
  "function": {
    "echofunction": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "firstrespondermobile",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput"
          ]
        }
      ]
    },
    "firstrespondercreatemeeting": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "firstresponderdbutillayer",
          "attributes": [
            "Arn"
          ]
        }
      ]
    },
    "firstresponderdbutillayer": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    }
  },
  "auth": {},
  "api": {
    "firstrespondermobile": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "First Responder Mobile API"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  }
}